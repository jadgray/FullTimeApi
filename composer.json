{
  "name": "jadgray/full-time-api",
  "description": "A simple PHP scraper for FA Full-Time Fixtures and results",
  "license": "MIT",
  "keywords": ["Football Association", "Football", "API", "Football Fixtures", "Football Results"],
  "type": "library",
  "authors": [
    {
      "name": "James Gray",
      "email": "github@gray.ac"
    }
  ],
  "support": {
    "issues": "https://github.com/jadgray/FullTimeApi/issues",
    "source": "https://github.com/jadgray/FullTimeApi"
  },
  "require": {
    "php": ">=7.2",
    "fabpot/goutte": "^3.2",
    "nesbot/carbon": "^1.39"
  },
  "autoload": {
    "psr-4": {
      "Jadgray\\FullTimeApi\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.3"
  }
}
